{
  "name": "gsheet-next-app",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 3000 -H 0.0.0.0",
    "lint": "next lint",
    "electron:dev": "concurrently -k \"npm:dev\" \"npm:electron:watch\"",
    "electron:watch": "set NODE_ENV=development&&electron .",
    "electron:build": "npm run build && electron-builder"
  },
  "homepage": "./",
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.6",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "googleapis": "^150.0.1",
    "jsonwebtoken": "^9.0.2",
    "next": "^16.1.6",
    "next-auth": "^4.24.11",
    "next-client-cookies": "^2.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "react-hot-toast": "^2.5.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4.1.8",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "build": {
    "appId": "com.gsheet.next.app",
    "productName": "gsheet-next-app",
    "copyright": "Copyright Â© 2024 ${author}",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "package.json",
      "out/**/*",
      "public/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
